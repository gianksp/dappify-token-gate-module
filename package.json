{
  "name": "dappify-token-gate-module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "source": "src/index.js",
  "alias": {
    "/": "./src/lib"
  },
  "scripts": {
    "test": "jest --collectCoverage=true",
    "coverage": "jest --coverage",
    "build": "parcel build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DappifyWeb3/dappify-token-gate-module.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DappifyWeb3/dappify-token-gate-module/issues"
  },
  "homepage": "https://github.com/DappifyWeb3/dappify-token-gate-module#readme",
  "devDependencies": {
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "parcel": "^2.7.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageReporters": [
      "json",
      "text",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/**/store.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!<rootDir>/node_modules/",
      "!**/templates/**",
      "!**/template/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
